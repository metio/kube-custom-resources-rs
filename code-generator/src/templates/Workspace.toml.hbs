[workspace]
members = [
{{#each groups as |group| }}
    "custom-resources/{{group}}",
{{/each}}
    "code-generator"
]
resolver = "2"

[profile.dev]
# Disable settings to speeds up builds
debug = false
lto = "off"
incremental = false
strip = "none"
debug-assertions = false
overflow-checks = false
